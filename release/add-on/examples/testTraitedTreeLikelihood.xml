<beast version='2.0'
       namespace='beast.evolution.alignment:beast.core:beast.evolution.tree:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>


    <!-- The sequence alignment                                                  -->
    <data id="alignment">
		<userDataType spec='beast.evolution.datatype.UserDataType' id='bdDataType' states='2'
			codeMap='0=1, 1=0, ?=0 1, -=0 1'
		/>
        <sequence taxon="German_ST">           ??????????????????????????????</sequence>
        <sequence taxon="Dutch_List">          ?????????????????????????????????????????????????</sequence>
        <sequence taxon="English_ST">          ?????????????????????????????????????????????</sequence>
        <sequence taxon="French">              </sequence>
        <sequence taxon="Italian">             ???????????</sequence>
        <sequence taxon="Spanish">             </sequence>
    </data>



	<userDataType spec='beast.evolution.datatype.UserDataType' id='traitdata' 
		states='2' 
		codelength='-1'
		codeMap='
		EuroZone = 1, 
		PoundZone = 0, 
		? = 0 1, 
		- = 0 1'
	/>

    <distribution spec='TraitedTreeLikelihood' id="traitedtreeLikelihood" dataType='@traitdata'>
        <traitSet id='traitset' spec='TraitSet' 
		 traitname='discrete'
         value="German_ST  = EuroZone, 
				Dutch_List = EuroZone, 
				French     = EUROZONE,
				English_ST = PoundZone,
				Spanish    = ?,
				Italian    = ?">
                <taxa spec='beast.evolution.alignment.TaxonSet' alignment='@alignment'/>
		</traitSet>
        <tree idref="tree"/>
        <siteModel idref="siteModel"/>
    </distribution>

    <distribution spec='TreeLikelihood' id="treeLikelihood" useAmbiguities="true">
        <data idref="alignment"/>
        <tree idref="tree"/>
        <siteModel spec='SiteModel' id="siteModel" gammaCategoryCount='1'>
            <substModel spec="MutationDeathModel" id="SDollo">
                <deathprob idref="cognate.loss"/>
                <frequencies id='freqs' spec='Frequencies'>
                        <frequencies id='frequencies' spec='parameter.RealParameter' value='1 0'/>
                </frequencies>
            </substModel>
        </siteModel>
    </distribution>

    <tree id='tree'/>


    <tree id='newicktree' spec='beast.util.TreeParser' taxa='@alignment' initial='@tree'
        newick='((English_ST:0.22743347188019544,(German_ST:0.10557648379843088,Dutch_List:0.10557648379843088):0.12185698808176457):1.5793160946109988,(Spanish:0.11078392189606047,(Italian:0.10119772534558173,French:0.10119772534558173):0.009586196550478737):1.6959656445951337)'/>
<!--
        newick='(English_ST:0.22743347188019544,(German_ST:0.10557648379843088,Dutch_List:0.10557648379843088):0.12185698808176457)'/>
-->

    <run spec="MCMC" id="mcmc" chainLength="1000000">
        <state storeEvery='100000'>
            <parameter name='stateNode' id="cognate.loss" value='1.7'/>
            <stateNode idref="frequencies"/>
            <stateNode idref="tree"/>
        </state>

		<init idref='newicktree'/>

		<distribution spec='CompoundDistribution' id='posterior'>
			<distribution spec='CompoundDistribution' id='prior'>
		        <distribution spec='beast.evolution.speciation.BirthDeathGernhard08Model' id="yulelike" type='unscaled' tree='@tree'>
		            <parameter name='birthDiffRate' id="birthRate" value="1.0" lower="0.0" upper="100.0"/>
		            <parameter name='relativeDeathRate' id='relativeDeathRate' value='0'/>
		        </distribution>
			</distribution>

			<distribution spec='CompoundDistribution' id='likelihood'>
				<distribution idref="traitedtreeLikelihood"/>
				<distribution idref="treeLikelihood"/>
			</distribution>

		</distribution>
<!--
        <operator id='shapeScaler' spec='ScaleOperator' scaleFactor="0.5" weight="1" parameter="@shape"/>
        <operator id='invarScaler' spec='ScaleOperator' scaleFactor="0.5" weight="1" parameter="@propInvar"/>
-->

        <operator id='treeScaler' spec='ScaleOperator' scaleFactor="0.5" weight="1" tree="@tree"/>


<!--
        <operator spec='SubtreeSlide' weight="5" gaussian="true" size="1.0" tree="@tree"/>
        <operator id="Reheight" spec="NodeReheight2" tree="@tree" weight="5" scaleFactor='0.5'>
        </operator>
-->

        <operator spec='Uniform' weight="10" tree="@tree"/>
        <operator spec='SubtreeSlide' weight="5" gaussian="true" size="1.0" tree="@tree"/>
        <operator id='narrow' spec='Exchange' isNarrow='true' weight="1" tree="@tree"/>
        <operator id='wide' spec='Exchange' isNarrow='false' weight="1" tree="@tree"/>
        <operator spec='WilsonBalding' weight="1" tree="@tree"/>

        <logger logEvery="10000" fileName="test.$(seed).log">
	        <model idref='posterior'/>
            <log idref="posterior"/>
            <log idref="prior"/>
            <log idref="likelihood"/>
            <log idref="traitedtreeLikelihood"/>
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
        </logger>
        <logger logEvery="10000" fileName="test.$(seed).trees">
            <log idref="tree"/>
        </logger>
        <logger logEvery="10000">
	        <model idref='posterior'/>
            <log idref="posterior"/>
            <log idref="likelihood"/>
            <log idref="traitedtreeLikelihood"/>
        </logger>
    </run>

</beast>
