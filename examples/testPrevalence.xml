<beast version='2.0'
       namespace='beast.prevalence:beast.core:beast.core.parameter:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>


    <!-- The sequence alignment                                                  -->
    <!-- ntax=6 nchar=768                                                        -->
    <!-- npatterns=69                                                            -->


    <data id="alignment" dataType="nucleotide">
        <sequence taxon="taxon_1" totalcount="4" value=""/>
        <sequence taxon="taxon_2" totalcount="4" value=""/>
        <sequence taxon="taxon_3" totalcount="4" value=""/>
        <sequence taxon="taxon_4" totalcount="4" value=""/>
        <sequence taxon="taxon_5" totalcount="4" value=""/>
        <sequence taxon="taxon_6" totalcount="4" value=""/>
        <sequence taxon="taxon_7" totalcount="4" value=""/>
        <sequence taxon="taxon_8" totalcount="4" value=""/>
        <sequence taxon="taxon_9" totalcount="4" value=""/>
        <sequence taxon="taxon_10" totalcount="4" value=""/>
    </data>

    <!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
    <input spec='HKY' id='hky'>
        <kappa idref='hky.kappa'/>
        <frequencies id='freqs' spec='Frequencies'>
            <data idref='alignment'/>
        </frequencies>
    </input>

    <!-- site model                                                              -->
    <input spec='SiteModel' id="siteModel">
        <substModel idref='hky'/>
    </input>

    <input spec='TreeLikelihood' id="treeLikelihood">
        <data idref="alignment"/>
        <tree idref="tree"/>
        <siteModel idref="siteModel"/>
    </input>

    <parameter id="hky.kappa" value="1.0" lower="0.0"/>

    <tree spec='beast.util.ClusterTree' id='tree' clusterType='upgma'>
        <taxa idref='alignment'/>
    </tree>

    


    <run spec="MCMC" id="mcmc" chainLength="100000">
        <state storeEvery='100000'>
            <stateNode spec='RealParameter' id="beta" value='1'/>
            <stateNode spec='RealParameter' id="gamma" value='.2'/>
            <stateNode spec='IntegerParameter' id="popSize" value='50'/>
            <stateNode spec='PrevalenceList' id='list'/>
            <stateNode idref="hky.kappa"/>
            <stateNode idref="tree"/>
        </state>

        <distribution spec='CompoundDistribution' id='posterior'>
            <distribution spec='CompoundDistribution' id='prior'>
                <!--distribution spec='PrevalenceLikelihood' id='prevlikelihood' beta='@beta' gamma='@gamma' popSize='@popSize' list='@list'/-->
                <distribution spec='TreeForPrevalenceLikelihood' id='treeprior' tree='@tree' list='@list'/>
            </distribution>
            <distribution id='likelihood' idref="treeLikelihood"/>
        </distribution>
    
        <operator id='addOperator' spec='AddOperator' weight='1' list='@list'/>
        <operator id='delOperator' spec='DelOperator' weight='1' list='@list'/>
        <operator id='moveOperator' spec='MoveOperator' weight='1' list='@list'/>

        <operator id='betaScaler' spec='beast.evolution.operators.ScaleOperator' scaleFactor="0.5" weight="1" parameter="@beta"/>
        <operator id='gammaScaler' spec='beast.evolution.operators.ScaleOperator' scaleFactor="0.5" weight="1" parameter="@gamma"/>
        <!--operator id='popSizeScaler' spec='beast.evolution.operators.ScaleOperator' scaleFactor="0.5" weight="1" parameter="@popSize"/-->

        <operator id='kappaScaler' spec='beast.evolution.operators.ScaleOperator' scaleFactor="0.5" weight="1" parameter="@hky.kappa"/>

        <operator id='narrow' spec='beast.evolution.operators.Exchange' isNarrow='true' weight="1" tree="@tree"/>
        <operator id='wide' spec='beast.evolution.operators.Exchange' isNarrow='false' weight="1" tree="@tree"/>
        <operator id='treeScaler' spec='TreeScaleOperator' scaleFactor="0.5" weight="1" tree="@tree" list='@list'/>
        <operator spec='Uniform' weight="10" tree="@tree" list='@list'/>
        <operator spec='SubtreeSlide' weight="5" gaussian="true" size="1.0" tree="@tree" list='@list'/>
        <operator spec='WilsonBalding' weight="1" tree="@tree" list='@list'/>

        <logger logEvery="10000" fileName="test.$(seed).log">
	        <model idref='posterior'/>
            <log idref="prior"/>
            <log idref="likelihood"/>
            <log idref="posterior"/>
            <log idref='hky.kappa'/>
            <log idref='beta'/>
            <log idref='gamma'/>
            <log idref='popSize'/>
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
        </logger>
        <logger logEvery="10000" fileName="test.$(seed).trees">
            <log idref="tree"/>
        </logger>

        <logger logEvery="1000">
	        <model idref='posterior'/>
            <log idref="treeprior"/>
            <!--log idref="prevlikelihood"/-->
            <log idref="prior"/>
            <log idref="likelihood"/>
            <log idref="posterior"/>
    	    <ESS spec='ESS' name='log' arg="@likelihood"/>
            <log idref='hky.kappa'/>
            <log idref='beta'/>
            <log idref='gamma'/>
            <log idref='popSize'/>
    	    <ESS spec='ESS' name='log' arg="@hky.kappa"/>
        </logger>
    </run>

</beast>
